{
  "name": "iso",
  "version": "0.3.324",
  "private": true,
  "scripts": {
    "format": "./node_modules/.bin/prettier --write ./*.js",
    "watch-css": "onchange 'css/*.less' -- npm run build-css",
    "watch-js": "./node_modules/.bin/watchify -v ./src/index.js -o ./js/min/iso.js -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] --plugins [ @babel/plugin-proposal-class-properties ] ]",
    "watch": "npm run watch-css & npm run watch-js",
    "build-css": "lessc --autoprefix='last 2 versions' css/style.less css/style.css",
    "build-js": "browserify src/index.js -o js/min/iso.js -t [ babelify --presets [ @babel/preset-env @babel/preset-react ] --plugins [ @babel/plugin-proposal-class-properties ] ]",
    "build": "npm run build-css && npm run build-js"
  },
  "author": "frankrowe",
  "dependencies": {
    "5to6-codemod": "^1.7.1",
    "brfs": "^1.4.0",
    "color": "^0.8.0",
    "csv2geojson": "^4.0.0",
    "envify": "^3.4.0",
    "filesaver.js": "^0.1.1",
    "fixed-data-table": "^0.6.0",
    "flux": "^2.0.1",
    "geojson-normalize": "0.0.0",
    "geojson2dsv": "0.0.0",
    "geojsonhint": "^1.0.0",
    "keymirror": "^0.1.1",
    "less": "^2.6.1",
    "less-plugin-autoprefix": "^1.5.1",
    "morgan": "~1.3.0",
    "numeral": "^1.5.3",
    "object-assign": "^2.0.0",
    "onchange": "^2.1.2",
    "osmtogeojson": "^2.2.5",
    "polytogeojson": "0.0.1",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "shp-write": "^0.2.1",
    "togeojson": "^0.11.1",
    "tokml": "^0.3.0",
    "topojson": "^1.6.18",
    "turf": "^2.0.2",
    "vinyl-source-stream": "^1.1.0",
    "wellknown": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "watchify": "^3.11.0"
  }
}
