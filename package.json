{
  "name": "iso",
  "version": "0.3.324",
  "private": true,
  "scripts": {
    "watch-css": "onchange 'css/*.less' -- npm run build-css",
    "watch-js": "watchify -v -t [ babelify --presets [ es2015 react ] ] ./src/index.js -o ./js/min/iso.js",
    "watch": "npm run watch-css & npm run watch-js",
    "build-css": "lessc --autoprefix='last 2 versions' css/style.less css/style.css",
    "build-js": "browserify --fast -t [ babelify --presets [ es2015 react ] ] src/index.js -o js/min/iso.js",
    "build": "npm run build-css && npm run build-js"
  },
  "author": "frankrowe",
  "dependencies": {
    "brfs": "^1.4.0",
    "color": "^0.8.0",
    "csv2geojson": "^4.0.0",
    "filesaver.js": "^0.1.1",
    "fixed-data-table": "^0.6.0",
    "flux": "^2.0.1",
    "geojson2dsv": "0.0.0",
    "geojsonhint": "^1.0.0",
    "keymirror": "^0.1.1",
    "morgan": "~1.3.0",
    "numeral": "^1.5.3",
    "object-assign": "^2.0.0",
    "osmtogeojson": "^2.2.5",
    "polytogeojson": "0.0.1",
    "shp-write": "^0.2.1",
    "togeojson": "^0.11.1",
    "tokml": "^0.3.0",
    "topojson": "^1.6.18",
    "turf": "^2.0.2",
    "wellknown": "^0.3.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^9.0.3",
    "envify": "^3.4.0",
    "geojson-normalize": "0.0.0",
    "less": "^2.6.1",
    "less-plugin-autoprefix": "^1.5.1",
    "onchange": "^2.1.2",
    "react": "^0.14.7",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
